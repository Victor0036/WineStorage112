<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.winestorage112">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.WineStorage112"
        android:usesCleartextTraffic="true">
        <activity android:name=".Detail"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".Edit"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".ui.AboutUs"
            android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".AddNote"
            android:parentActivityName=".MainActivity"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.SignInActivity"></activity>
    </application>

</manifest>